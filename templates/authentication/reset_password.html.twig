{% extends 'base.html.twig' %}

{% block title %}
    Connexion
{% endblock %}

{% block body %}
    <div class="container">
        <div class="row">
            <div class="col-md-4 offset-md-4 ">
                <form method="post">
                    <label for="pass">Mot de passe </label>
                    <input type="password" name="password" id="pass">
                    <input type="hidden" name="token" value="{{ token }}">
                    <button>Valider</button>
                </form>
            </div>
        </div>
    </div>
{% endblock %}





