{% extends 'base.html.twig' %}

{% block title %}
    <h1>Création d'une catégorie</h1>
{% endblock %}
{% block body %}
    <h1>Créer une catégorie</h1>
    {{ form_start(formView) }}
    {{ form_widget(formView) }}
    <button type="submit" class="btn btn-primary">
        <i class="fas fa-save"></i>
        Créer une categorie
    </button>
    {{ form_end(formView) }}
{% endblock %}